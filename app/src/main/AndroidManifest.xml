        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.movimiento.MovimientosActivity"></activity>
        <activity android:name=".view.movimiento.MovimientoActivity" />
        <activity android:name=".view.ActualizacionTarifaActivity" />
